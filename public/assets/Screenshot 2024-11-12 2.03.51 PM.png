body{
    margin:0px;
    padding:0px;
    font-family:tahoma;
    background-size: cover;
    display: inline-block;
}
section {
  display: inline-block;
}
.header {
    position: fixed;
    background-color: rgba(166,54,134,0.9);
    border-radius: 10px;
    width: 100%;
    display: inline-block;
}
#search {
    align-items: center;
    justify-content: center;
    margin-left: auto;
    margin-right: auto;
    text-align: center;
    margin-top: 100px;
    height: 50px;
    width: 50vw;
    border-radius: 50px;
    background-color: rgba(166,54,134,0.9);
    border: solid 5px;
    border-color: rgba(166,54,134,1);
}
#search::placeholder {
    color: grey;
}
.temp123{
  text-align:center;
}
.temp123 > input{
  text-align:left;
}
.games {
    margin-top: 75px;
}
.game-button {
    aspect-ratio: 1;
    width: 115px;
    border: 10px 10px black;
    border-radius: 5px;
    padding-top: 65px;
    display: inline-block;
    transition: all 0.5s ease-in-out;
    color: white;
    background-size: cover;
    margin-top: 5px;
    margin-left: 20px;
    white-space: nowrap;
    text-align: center;
}
.game-button:hover {
    transform: scale(1.1);
}
.left-header {
    display: inline-block;
}
.right-header {
    display: inline-block;
    margin-left: 75%;
}